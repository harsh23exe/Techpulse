{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://real-time-news-agent.vercel.app",
    "NEXT_PUBLIC_WS_URL": "wss://real-time-news-agent.vercel.app"
  },
  "functions": {
    "app/api/backend/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/backend/:path*",
      "destination": "https://real-time-news-agent.vercel.app/:path*"
    }
  ]
} 